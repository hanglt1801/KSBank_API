// Ngày hiện tại
LocalDate localDate = LocalDate.now();
String today = DateTimeFormatter.ofPattern(&quot;dd/MM/yyyy&quot;).format(localDate);
props.put(&quot;today&quot;, today);

// Ngày hiện tại - 7 ngày
LocalDate minusDays = LocalDate.now().minusDays(7);
String minusDays7 = DateTimeFormatter.ofPattern(&quot;dd/MM/yyyy&quot;).format(minusDays);
props.put(&quot;minusDays7&quot;, minusDays7);

// Ngày hiện tại + 1 ngày
LocalDate plusDay1 = LocalDate.now().plusDays(1);
String pl1 = DateTimeFormatter.ofPattern(&quot;dd/MM/yyyy&quot;).format(plusDay1);
props.put(&quot;test.today1&quot;, pl1); 
// Ngày hiện tại + 7 ngày
LocalDate plusDay7 = LocalDate.now().plusDays(7);
String pl7 = DateTimeFormatter.ofPattern(&quot;dd/MM/yyyy&quot;).format(plusDay7);
props.put(&quot;test.today7&quot;, pl7);
// Ngày hiện tại + 30 ngày
LocalDate plusDay30 = LocalDate.now().plusDays(30);
String pl30 = DateTimeFormatter.ofPattern(&quot;dd/MM/yyyy&quot;).format(plusDay30);
props.put(&quot;test.today30&quot;, pl30);

//Ngày hiện tại với định dạng ngày tháng, giờ phút
String todayDateTime = LocalDateTime.now();
props.put(&quot;todayDateTime&quot;,todayDateTime );

// Ngày đầu tiên của tháng
LocalDate day1 = LocalDate.now().withDayOfMonth(1);
String Day1 = DateTimeFormatter.ofPattern(&quot;dd/MM/yyyy&quot;).format(day1);
props.put(&quot;firstDay&quot;,Day1 );

// Ngày cuối cùng của tháng
LocalDate endOfMonth = LocalDate.now().with(TemporalAdjusters.lastDayOfMonth());
String DayEOM = DateTimeFormatter.ofPattern(&quot;dd/MM/yyyy&quot;).format(endOfMonth);
props.put(&quot;endDay&quot;,DayEOM );


</stringProp>
