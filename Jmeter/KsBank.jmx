log.info(&quot;TC - ${test.id} - ${test.objective}&quot;);
log.info(&quot;Số tiền trước thanh toán        : &quot;+ balance_before);
log.info(&quot;Số tiền thanh toán              : &quot;+ transfer_amount);
log.info(&quot;Số tiền thực tế sau thanh toán  : &quot;+ actual_amount);
log.info(&quot;Số tiền mong muốn sau thanh toán: &quot;+ expected_amount);

if(expected_amount != actual_amount){
      AssertionResult.setFailure(true);  
      AssertionResult.setFailureMessage(&quot;Sai thông tin số dư tài khoản sau thanh toán Expected amount = &quot;+expected_amount + &quot;Actual amount = &quot;+ actual_amount);
}
else{
