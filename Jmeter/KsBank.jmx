              <stringProp name="script">// Chuẩn set dữ liệu Ngày đặt, ngày kết thúc
public String formatDate(Date dateTime) {
	Calendar cal = Calendar.getInstance();
	cal.setTime(dateTime);
	// dd/MM/yyyy
	StringBuffer buff = new StringBuffer();
	buff.append(cal.get(Calendar.DAY_OF_MONTH) &lt; 10 ? &quot;0&quot; + cal.get(Calendar.DAY_OF_MONTH) : cal.get(Calendar.DAY_OF_MONTH)).append(&quot;/&quot;);
	buff.append((cal.get(Calendar.MONTH) + 1) &lt; 10 ? &quot;0&quot; + (cal.get(Calendar.MONTH) + 1) : (cal.get(Calendar.MONTH) + 1)).append(&quot;/&quot;);
	buff.append(cal.get(Calendar.YEAR));
}

public Date adjustDate(Date dateTime, int numberOfDay) {
	Calendar cal = Calendar.getInstance();
	cal.setTime(dateTime);
	cal.add(Calendar.DAY_OF_MONTH, numberOfDay);
	return cal.getTime();
	
}


String FROM = formatDate(new Date());
Date TO = adjustDate(new Date(), 7);
String formatTo = formatDate(TO);
// Ngày hiện tại + 1 ngày
Date tomorrow = adjustDate(new Date(), 1);
String formatTomorrow = formatDate(tomorrow);
vars.put(&quot;test.tomorrow&quot;,formatTomorrow);
// Ngày hiện tại + 8 ngày
Date  todayAdd8= adjustDate(new Date(), 8);
String formatTodayAdd8 = formatDate(todayAdd8);
vars.put(&quot;test.endDay&quot;,formatTodayAdd8);
// Ngày hiện tại + 17 ngày
Date  todayAdd30= adjustDate(new Date(), 30);
String formatTodayAdd30 = formatDate(todayAdd30);
vars.put(&quot;test.endWeek&quot;,formatTodayAdd30);

// Ngày hiện tại + 60 ngày
Date  todayAdd60= adjustDate(new Date(), 60);
String formatTodayAdd60 = formatDate(todayAdd60);
vars.put(&quot;test.endMonth&quot;,formatTodayAdd60);

log.info(&quot;FROM:  &quot; + FROM + &quot; TO: &quot; + formatTo);
log.info(&quot;Ngày hiện tại: &quot;+ FROM);
log.info(&quot;Ngày hiện tại + 1 ngày:  &quot;+ formatTomorrow);
log.info(&quot;Ngày hiện tại + 8 ngày:  &quot;+ formatTodayAdd8);
log.info(&quot;Ngày hiện tại + 30 ngày:  &quot;+ formatTodayAdd30);
log.info(&quot;Ngày hiện tại + 60 ngày:  &quot;+ formatTodayAdd60);




